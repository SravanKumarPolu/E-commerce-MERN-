function getCartData() {
    jQuery.ajax({
        url: "https://www.ubuy.co.in/ubuycom/pages/cft-v1/?s_id=31&type=19&id=adb5770f83c0874818f11999f8bee925",
        dataType: 'html',
        type: 'GET',
        data: "",
        success: function(resp) {
            var menuHtml = resp;
            if (jQuery.trim(menuHtml) != '') {
                var obj = jQuery.parseJSON(menuHtml);
                if (jQuery.trim(obj.userAcountHtml) != '') {
                    jQuery(".header-account").html(obj.userAcountHtml);
                }
                if (jQuery.trim(obj.reviewForm) != '') {
                    jQuery("#review_form").html(obj.reviewForm);
                }
                jQuery("#user_cart_count").html(obj.cartcount);
                jQuery("#user_cart_count_sm").html(obj.cartcount);
                if (obj.cartcount > 0) {
                    jQuery("#user_cart_count").addClass("active");
                } else {
                    jQuery("#user_cart_count").removeClass("active");
                }
            }
        },
        complete: function() {}
    });
}

function changeStoreEvent(swap_store, swap_url) {
    var customer_id = '';
    if (typeof changeStore != "undefined") {
        var store_data = {};
        user_type = (customer_id != "") ? "registered" : "guest";
        store_data.old_store = 'us';
        store_data.new_store = swap_store
        store_data.user_id = customer_id
        store_data.user_type = user_type;
        store_data.swap_url = swap_url;
        changeStore(store_data);
    }
}

jQuery(document).ready(function() {
    getCartData();
    var current_store = 'us';
    var current_active_store = 'us';
    console.log(current_active_store, current_store);
    updateCurrentStoreNode(current_store);
    //if (current_store == 'eu') {
    //   current_store = 'de';
    //}
    jQuery("#mobile-stores").find('li[data-astore="' + current_active_store + '"]').addClass('active-store');
    //jQuery('span[data-store="' + current_store + '"]').addClass('active');
    //jQuery('span[data-store="' + current_store + '"]').removeClass('storeswap');
    jQuery('span[data-store="' + current_active_store + '"]').addClass('active');
    jQuery('span[data-store="' + current_active_store + '"]').removeClass('storeswap');
    jQuery(document).on("click", ".storeswap", function() {
        if (jQuery(this).hasClass('active')) {
            return;
        }

        var is_store_change =
            '1';
        var store = jQuery(this).attr('data-store');
        var mstore = jQuery(this).attr('data-mstore');
        var url = jQuery(this).attr('data-href');
        if (mstore == "de") {
            mstore = 'eu';
        }
        var is_popup = false;
        if (store != current_store) {
            var is_popup = true;
            if (mstore == 'us' && jQuery.inArray(current_store, ['us', 'fashion', 'mart', 'used']) !=
                '-1') {
                is_popup = false;
            } else if (mstore == 'uk' && current_store == 'uk') {
                is_popup = false;
            } else if (mstore == 'ch' && current_store == 'ch') {
                is_popup = false;
            }
        }
        if (is_popup) {
            var base_icon = 'https://d3ulwu8fab47va.cloudfront.net/skin/frontend/default/ubuycom-v1/images/countries-flag/';
            var from_icon = base_icon + "new-" + current_active_store +
                "-icon.png";
            var to_icon = base_icon + "new-" + mstore + "-icon.png";
            var url_tag = '<button id="store-swap-link"  data-swap_store="' + store +
                '" data-swap_url="' + url +
                '" class="btn btn-primary text-dark mb-2 mt-2 mr-1"><strong>YES</strong></button>';
            var store_change_msg = '';
            jQuery("#store-change-from-icon").attr("src", from_icon);
            jQuery("#store-change-from-text").html(current_active_store.toUpperCase());
            jQuery("#store-change-to-icon").attr("src", to_icon);
            jQuery("#store-change-to-text").html(mstore.toUpperCase());
            jQuery("#store-swap-link").attr('href', url);
            jQuery("#store-swap-link").after(url_tag);
            jQuery("#store-swap-link").last().remove();

            if (jQuery("#store-change-popup-messgae").length > 0 && jQuery(
                    "#store-change-popup-messgae").text().indexOf('Products shown in other markets') <
                1) {

                jQuery("#store-change-popup-messgae").append(store_change_msg);
                store_change_msg = '';
            }
            if (is_store_change > 0) {
                window.location.href = url;
            } else {
                jQuery("#storeChangeModal").modal('show');
            }
        } else {
            window.location.href = url;
        }
    });

    jQuery(document).on('click', "#store-swap-link", function() {
        showLoaderImage();
        var swap_url = jQuery(this).data("swap_url");
        var swap_store = jQuery(this).data("swap_store");
        updateCurrentStoreNode(swap_store);
        changeStoreEvent(swap_store, swap_url);
        jQuery("#storeChangeModal").modal('hide');

        window.location.href = swap_url;
    });
    jQuery(document).on('click', "#store-swap-link-hide", function() {

        jQuery(".body-desktop,.body-android,.body-iphone,.cart").removeClass('stuck-here');
        jQuery(".body-desktop,.body-android,.body-iphone,.cart").attr('style', '');
    });

    //

    if (jQuery(".show-cart-popup").length > 0 && screen.width <= 768) {
        jQuery(".show-cart-popup").removeClass('show-cart-popup');
    }
});

