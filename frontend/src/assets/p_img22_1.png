function updateCurrentStoreNode(storeName) {
    if (storeName != null) {
        let store = JSON.stringify({
            code: storeName.toLowerCase(),
            title: storeName.toUpperCase()
        });
        localStorage.setItem('currentStore', store);
    }
}
