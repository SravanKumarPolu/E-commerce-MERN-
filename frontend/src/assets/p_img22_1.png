                if (jQuery("#product-attribute-specs-table").length > 0 && weight != undefined) {
                    if (!jQuery("#product-attribute-specs-table td:contains('Item Weight')").length) {
                        jQuery("#product-attribute-specs-table").append("<tr><td>Item Weight</td><td>" + weight + " lbs ("+ (weight * 0.45).toFixed(2) +" Kg)</td></tr>");
                    }
                } else if (jQuery(".product-detail-section").length > 0 && weight != undefined) {
                    if (!jQuery(".product-detail-section ul li:contains('Item Weight')").length) {
                        jQuery(".product-detail-section ul").append("<li>Item Weight: " + weight + " lbs ("+ (weight * 0.45).toFixed(2) +" Kg)</li>");
                    }
                }
                if(jQuery('.free-shipping-note').length && weight > 1){
                    jQuery('.free-shipping-note').remove();
                }
                
            }
        });
    }
    
